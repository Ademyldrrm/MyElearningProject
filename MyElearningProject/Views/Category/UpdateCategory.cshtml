@model MyElearningProject.DAL.Entities.Category
@{
    ViewBag.Title = "UpdateCategory";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Kategori Güncelleme Syfası</h2>

<form method="post">

    <label>Kategori ID</label>
    @Html.TextBoxFor(x =>x.CategoryID, new { @class = "form-control" })
    <label>Kategori Adı</label>
    @Html.TextBoxFor(x => x.CategoryName, new { @class = "form-control" })
   
    <button class="btn btn-outline-success">Kategorileri  Güncelle</button>
</form>